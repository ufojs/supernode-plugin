{
  "manifest_version": 2,
  "name": "Ufo.js supernode chrome app",
  "version": "1",
  "minimum_chrome_version": "33",
  "app": {
    "background": {
      "scripts": ["supernode.bundle.min.js"]
    }
  },
  "externally_connectable": {
    "matches": ["<all_urls>"]
  },
  "sockets": {
    "tcpServer": {
      "listen": "*"
    },
    "tcp": {
      "connect": "*"
    }
  }
}
