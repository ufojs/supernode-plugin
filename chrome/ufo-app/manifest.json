{
  "manifest_version": 2,
  "name": "Ufo.js supernode chrome app",
  "version": "1",
  "minimum_chrome_version": "33",
  "app": {
    "background": {
      "scripts": ["supernode.bundle.min.js", "app.js"]
    }
  },
  "externally_connectable": {
    "matches": ["*://*.google.it/*", "http://localhost:9876/*"]
  },
  "key": "",
  "sockets": {
    "tcpServer": {
      "listen": "*"
    },
    "tcp": {
      "connect": "*"
    }
  }
}
