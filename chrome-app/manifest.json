{
  "manifest_version": 2,
  "name": "Ufo.js supernode plugin",
  "version": "1",
  "minimum_chrome_version": "33",
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "permissions": [
    "tabs"
  ],
  "sockets": {
    "tcpServer": {
      "listen": "*"
    },
    "tcp": {
      "connect": "*"
    }
  }
}
